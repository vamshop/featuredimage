{
    "name": "vamshop/featuredimage",
    "description": "Featured image plugin for VamShop",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": "~3.2",
        "vamshop/vamshop-core": "3.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "FeaturedImage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FeaturedImage\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
